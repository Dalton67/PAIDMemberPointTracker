<%= link_to("<< Back to List", events_path, :class => 'back-link') %>

<div class="members show">
  <h2>Show Event</h2>

  <table summary="Event detail view">
    <tr>
      <th>ID</th>
      <td><%= @events.id %></td>
    </tr>
    <tr>
      <th>Title</th>
      <td><%= @events.title %></td>
    </tr>
    <tr>
      <th>Points</th>
      <td><%= @events.points_worth %></td>
    </tr>
    <tr>
      <th>Semester</th>
      <td><%= @events.semester %></td>
    </tr>
  </table>

</div>
